<XMLDB xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" PATH="mod/assign/submission/changes/db" VERSION="20170815" COMMENT="XMLDB file for Moodle mod/assign/submission/changes" xsi:noNamespaceSchemaLocation="../../../../../lib/xmldb/xmldb.xsd">
  <TABLES>
    <TABLE NAME="assignsubmission_changes" COMMENT="Info about detected changes">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="submission" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="author" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false"/>
        <FIELD NAME="changes" TYPE="text" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timestamp" TYPE="int" LENGTH="11" NOTNULL="false" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="The unique id for this change."/>
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
